                              "Key": "EndDate",
                              "Value": "1-mar"
                            },
                            {
                              "Key": "FASWThreshold",
                              "Value": "0.7"
                            },
                            {
                              "Key": "FASWDepth",
                              "Value": "2000"
                            },
                            {
                              "Key": "Amount",
                              "Value": "20"
                            }
                          ],
                          "Name": "AutoIrrigationBasedOnWaterDeficit",
                          "IncludeInDocumentation": true,
                          "Enabled": true,
