                  "$type": "Models.PMF.Plant, Models",
                  "PlantType": null,
                  "IsAlive": false,
                  "IsEnding": false,
                  "DaysAfterEnding": 0,
                  "ResourceName": null,
                  "Name": "Grapevine",
                  "Children": [],
