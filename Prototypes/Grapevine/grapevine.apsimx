                            },
                            {
                              "Key": "FASWThreshold",
                              "Value": "0.6"
                            },
                            {
                              "Key": "FASWDepth",
                              "Value": "1500"
                            },
                            {
                              "Key": "Amount",
                              "Value": "20"
                            }
                          ],
                          "Name": "AutoIrrigationBasedOnWaterDeficit",
                          "IncludeInDocumentation": true,
                          "Enabled": true,
                          "ReadOnly": false
