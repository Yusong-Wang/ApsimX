              "Key": "StartDate",
              "Value": "1-nov"
            },
            {
              "Key": "EndDate",
              "Value": "1-mar"
            },
            {
              "Key": "FASWThreshold",
              "Value": "0.8"
            },
            {
              "Key": "FASWDepth",
              "Value": "2000"
            },
            {
              "Key": "Amount",
              "Value": "20"
            }
          ],
          "Name": "AutoIrrigationBasedOnWaterDeficit",
