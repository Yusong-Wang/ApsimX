                            {
                              "Key": "ResetNutrients",
                              "Value": "Yes"
                            },
                            {
                              "Key": "ResetSOM",
                              "Value": "Yes"
                            }
                          ],
                          "Name": "Reset on date",
                          "IncludeInDocumentation": true,
                          "Enabled": true,
                          "ReadOnly": false
